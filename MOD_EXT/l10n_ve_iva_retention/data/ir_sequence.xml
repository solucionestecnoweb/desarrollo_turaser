<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_iva_retention_supplier" model="ir.sequence">
            <field name="name">Retention IVA - Supplier - VE</field>
            <field name="code">iva.retention.supplier</field>
            <field name="implementation">standard</field>
            <field name="prefix">%(year)s%(month)s</field>
            <field name="padding">8</field>
            <field name="active" eval="1"/>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_iva_retention_customer" model="ir.sequence">
            <field name="name">Retention IVA - Customer - VE</field>
            <field name="code">iva.retention.customer</field>
            <field name="implementation">standard</field>
            <field name="prefix">%(year)s%(month)s</field>
            <field name="padding">8</field>
            <field name="active" eval="1"/>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>