<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_product_marca_vehiculo_form" model="ir.ui.view">
        <field name="name">view.product.marca.vehiculo.form</field>
        <field name="model">product.marca.vehiculo</field>
        <field name="arch" type="xml">
            <form string="Marca Vehiculo">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_product_marca_vehiculo_tree" model="ir.ui.view">
        <field name="name">view.product_marca.vehiculo.tree</field>
        <field name="model">product.marca.vehiculo</field>
        <field name="arch" type="xml">
            <tree string="Marca Vehiculo">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="action_product_marca_vehiculo" model="ir.actions.act_window">
        <field name="name">Marca Vehiculo</field>
        <field name="res_model">product.marca.vehiculo</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>