<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_product_otras_aplicaciones_form" model="ir.ui.view">
        <field name="name">view.product.otras.aplicaciones.form</field>
        <field name="model">product.otras.aplicaciones</field>
        <field name="arch" type="xml">
            <form string="Otras Aplicaciones">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_product_otras_aplicaciones_tree" model="ir.ui.view">
        <field name="name">view.product.otras.aplicaciones.tree</field>
        <field name="model">product.otras.aplicaciones</field>
        <field name="arch" type="xml">
            <tree string="Otras Aplicaciones">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="action_product_otras_aplicaciones" model="ir.actions.act_window">
        <field name="name">Otras Aplicaciones</field>
        <field name="res_model">product.otras.aplicaciones</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>