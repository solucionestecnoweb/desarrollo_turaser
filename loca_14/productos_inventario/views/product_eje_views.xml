<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_eje_form" model="ir.ui.view">
        <field name="name">view.eje.form</field>
        <field name="model">product.eje</field>
        <field name="arch" type="xml">
            <form string="Eje Carro">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_eje_tree" model="ir.ui.view">
        <field name="name">view.eje.tree</field>
        <field name="model">product.eje</field>
        <field name="arch" type="xml">
            <tree string="Eje Carro">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="action_product_eje" model="ir.actions.act_window">
        <field name="name">Eje Carro</field>
        <field name="res_model">product.eje</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>