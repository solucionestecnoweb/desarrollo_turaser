<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_product_medida_form" model="ir.ui.view">
        <field name="name">view.product.medida.form</field>
        <field name="model">product.medida</field>
        <field name="arch" type="xml">
            <form string="Medida">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_product_medida_tree" model="ir.ui.view">
        <field name="name">view.product.medida.tree</field>
        <field name="model">product.medida</field>
        <field name="arch" type="xml">
            <tree string="Medida">
                <field name="name"/>
            </tree>
        </field>
    </record>
    <record id="action_product_medida" model="ir.actions.act_window">
        <field name="name">Producto Medida</field>
        <field name="res_model">product.medida</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>