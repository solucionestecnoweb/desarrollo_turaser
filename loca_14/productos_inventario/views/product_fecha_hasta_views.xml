<odoo>
    <record id="view_product_year_hasta_form" model="ir.ui.view">
        <field name="name">view.year.hasta.form</field>
        <field name="model">product.year.hasta</field>
        <field name="arch" type="xml">
            <form string="Fecha Hasta">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_year_hasta_tree" model="ir.ui.view">
        <field name="name">view.year.hasta.tree</field>
        <field name="model">product.year.hasta</field>
        <field name="arch" type="xml">
            <tree string="Fecha Hasta">
                <field name="name"/>
            </tree>
        </field>
    </record>

