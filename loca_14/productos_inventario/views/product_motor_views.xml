<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_product_motor_form" model="ir.ui.view">
        <field name="name">view.product.motor.form</field>
        <field name="model">product.motor</field>
        <field name="arch" type="xml">
            <form string="Motor Cilindro">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_product_motor_tree" model="ir.ui.view">
        <field name="name">view.product.motor.tree</field>
        <field name="model">product.motor</field>
        <field name="arch" type="xml">
            <tree string="Motor Cilindro">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="action_product_motor" model="ir.actions.act_window">
        <field name="name">Motor</field>
        <field name="res_model">product.motor</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>