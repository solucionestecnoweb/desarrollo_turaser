<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_product_modelo_form" model="ir.ui.view">
        <field name="name">view.product.modelo.form</field>
        <field name="model">product.modelo</field>
        <field name="arch" type="xml">
            <form string="Modelo Carro">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_product_modelo_tree" model="ir.ui.view">
        <field name="name">view.product.modelo.tree</field>
        <field name="model">product.modelo</field>
        <field name="arch" type="xml">
            <tree string="Modelo Carro">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="action_product_modelo" model="ir.actions.act_window">
        <field name="name">Modelo</field>
        <field name="res_model">product.modelo</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>